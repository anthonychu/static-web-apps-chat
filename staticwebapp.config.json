{
    "routes": [
        {
            "route": "/.auth/login/twitter",
            "statusCode": "404"
        },
        {
            "route": "/.auth/login/facebook",
            "statusCode": "404"
        },
        {
            "route": "/.auth/login/google",
            "statusCode": "404"
        },
        {
            "route": "/.auth/login/aad",
            "statusCode": "404"
        },
        {
            "route": "/api/*",
            "allowedRoles": [
                "authenticated"
            ]
        }
    ],
    "responseOverrides": {
        "404": {
            "rewrite": "/custom-404.html",
            "statusCode": 200
        }
    }
}