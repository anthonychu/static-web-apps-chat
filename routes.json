{
  "routes": [
    {
      "route": "/login",
      "serve": "/.auth/login/twitter",
      "statusCode": 302
    },
    {
      "route": "/logout",
      "serve": "/.auth/logout",
      "statusCode": 302
    },
    {
      "route": "/.auth/login/github",
      "statusCode": "404"
    },
    {
      "route": "/.auth/login/aad",
      "statusCode": "404"
    },
    {
      "route": "/.auth/login/facebook",
      "statusCode": "404"
    },
    {
      "route": "/.auth/login/google",
      "statusCode": "404"
    },
    {
      "route": "/api/*",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/",
      "allowedRoles": ["authenticated"]
    }
  ]
}